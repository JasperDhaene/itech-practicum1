<%= form_for :events do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
  
    <p>
    <%= f.label :start %><br>
    <%= f.text_area :start %>
  </p>
  
  <p>
    <%= f.label :end %><br>
    <%= f.text_area :end %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>