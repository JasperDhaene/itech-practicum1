<%= form_for @event do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
  
    <p>
    <%= f.label :startdate %><br>
    <%= f.date_select :startdate %>
  </p>
  
  <p>
    <%= f.label :enddate %><br>
    <%= f.date_select :enddate %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
